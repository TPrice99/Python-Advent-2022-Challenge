while found==False:
  if len(marker) < 14:
    marker.append(line[i])
    i+=1
  #print("new 4",marker)
  
  n=0
  win=0
  while len(marker) == 14: 
    l=0
    list=[]
    while len(list)<len(marker):
      list.append(marker[l])
      l+=1

    list.pop(n)
    print(list, marker)
    
    if marker[n] in list:
      #list.append(marker[n])
      marker.pop(0)
      #print("in marker", marker)
      n=20
